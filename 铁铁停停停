--Debug Print
local currVersion = "v1.4"

_G.fishdebug = true
function dprint(str)
	if _G.fishdebug then
		game:GetService('TestService'):Message("[Fish Sim GUI] "..str)
	end
end

--Load GUI
local library = loadstring(game:HttpGet("https://pastebin.com/raw/djhHQFXz", true))()
